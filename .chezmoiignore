.sqlite_history
.lesshst
.viminfo

# Ignore credentials and tokens
.claude.json
.claude.json.backup
.emulator_console_auth_token

# Ignore cache directories
.cache/

# Ignore local secrets (if you create .zshrc.local later)
.zshrc.local
.env.local
.secrets

# Ignore specific application data
.wakatime.bdb
